{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"outDir": "build"
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/**/*.ts"
	]
}